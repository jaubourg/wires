{
    "author": "Julian Aubourg <j@ubourg.net> (http://jaubourg.net)",
    "bin": {
        "wires": "./lib/bin.js"
    },
    "description": "simple configuration utility with smart module wiring for unobtrusive dependency injection",
    "devDependencies": {
        "coveralls": "2.13.1",
        "eslint": "4.6.1",
        "eslint-config-creative-area": "0.7.0",
        "fs-extra": "4.0.1",
        "istanbul": "0.4.5",
        "nodeunit": "0.11.1"
    },
    "engines": {
        "node" : ">=4"
    },
    "eslintConfig": {
        "extends": "creative-area/es6",
        "env": {
            "node": true
        },
        "rules": {
            "global-require": "off",
            "no-sync": "off",
            "prefer-destructuring": "off",
            "prefer-rest-params": "off"
        }
    },
    "license": "MIT",
    "main": "./lib/index.js",
    "name": "wires",
    "repository" : {
        "type" : "git",
        "url" : "http://github.com/jaubourg/wires.git"
    },
    "scripts": {
        "cover": "node test cover",
        "coveralls": "npm run cover && cat ./coverage/lcov.info | coveralls",
        "test": "node test"
    },
    "version": "2.0.0-pre"
}
